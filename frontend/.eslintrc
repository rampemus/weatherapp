{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    'indent': [
            'error',
            2
        ],
  },
  "globals": {
    "fetch": false,
    "document": true
  }
}
